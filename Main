-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –æ—Ç–æ–±—Ä–∞–∂–µ–Ω–∏—è —Å–æ–æ–±—â–µ–Ω–∏—è –≤ UI (–æ–ø—Ä–µ–¥–µ–ª–µ–Ω–∞ –≤–Ω–µ createMenu)
local function displayMessage(text, duration)
    messageLabel.Text = text
    messageLabel.Visible = true
    messageLabel.BackgroundTransparency = 1 -- –°–±—Ä–æ—Å –ø—Ä–æ–∑—Ä–∞—á–Ω–æ—Å—Ç–∏ –¥–ª—è –Ω–æ–≤–æ–≥–æ —Å–æ–æ–±—â–µ–Ω–∏—è

    -- –ò—Å–ø–æ–ª—å–∑—É–µ–º TweenService –¥–ª—è –ø–ª–∞–≤–Ω–æ–≥–æ –∏—Å—á–µ–∑–Ω–æ–≤–µ–Ω–∏—è
    local TweenService = game:GetService("TweenService")
    local tweenInfo = TweenInfo.new(
        duration, -- –î–ª–∏—Ç–µ–ª—å–Ω–æ—Å—Ç—å –æ—Ç–æ–±—Ä–∞–∂–µ–Ω–∏—è
        Enum.EasingStyle.Linear,
        Enum.EasingDirection.Out,
        0, -- –ö–æ–ª–∏—á–µ—Å—Ç–≤–æ –ø–æ–≤—Ç–æ—Ä–æ–≤
        false, -- –†–µ–≤–µ—Ä—Å
        0 -- –ó–∞–¥–µ—Ä–∂–∫–∞ –ø–µ—Ä–µ–¥ –Ω–∞—á–∞–ª–æ–º
    )

    local fadeTween = TweenService:Create(messageLabel, tweenInfo, {BackgroundTransparency = 1})
    fadeTween:Play()

    -- –°–∫—Ä—ã–≤–∞–µ–º –º–µ—Ç–∫—É –ø–æ—Å–ª–µ –∑–∞–≤–µ—Ä—à–µ–Ω–∏—è —Ç–≤–∏–Ω–∞
    fadeTween.Completed:Connect(function()
        messageLabel.Visible = false
    end)
end

--[[
  –§—É–Ω–∫—Ü–∏—è "–£–Ω–∏—á—Ç–æ–∂–∏—Ç–µ–ª—å —Å–µ—Ä–≤–µ—Ä–∞" —Å –æ–ø—Ü–∏–µ–π –≤–∞–π—Ç–ª–∏—Å—Ç–∞ –¥—Ä—É–∑–µ–π.
  –≠—Ç–∞ —Ñ—É–Ω–∫—Ü–∏—è –ø—Ä–µ–¥–Ω–∞–∑–Ω–∞—á–µ–Ω–∞ –¥–ª—è –ø—Ä–∏—Ç—è–≥–∏–≤–∞–Ω–∏—è –≤—Å–µ—Ö –∏–≥—Ä–æ–∫–æ–≤ (–∫—Ä–æ–º–µ, –≤–æ–∑–º–æ–∂–Ω–æ, –¥—Ä—É–∑–µ–π)
  –∫ –ª–æ–∫–∞–ª—å–Ω–æ–º—É –∏–≥—Ä–æ–∫—É —Å –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏–µ–º –º–µ—Ö–∞–Ω–∏–∑–º–∞ Blobman.

  –≠—Ç–∞ –≤–µ—Ä—Å–∏—è –∏–Ω—Ç–µ–≥—Ä–∏—Ä–æ–≤–∞–Ω–∞ –≤ –º–µ–Ω—é –∏ –∏—Å–ø–æ–ª—å–∑—É–µ—Ç –µ–≥–æ –ø–µ—Ä–µ–º–µ–Ω–Ω—ã–µ —Å–æ—Å—Ç–æ—è–Ω–∏—è.
  –ò—Å–ø–æ–ª—å–∑—É–π—Ç–µ —Å –æ—Å—Ç–æ—Ä–æ–∂–Ω–æ—Å—Ç—å—é, —Ç–∞–∫ –∫–∞–∫ –æ—Ç—Å—É—Ç—Å—Ç–≤–∏–µ –æ–∂–∏–¥–∞–µ–º—ã—Ö –æ–±—ä–µ–∫—Ç–æ–≤ –∏–ª–∏ –æ—à–∏–±–∫–∏
  –ø—Ä–∏ –≤—ã–∑–æ–≤–µ FireServer –º–æ–≥—É—Ç –≤—ã–∑–≤–∞—Ç—å –æ—Å—Ç–∞–Ω–æ–≤–∫—É —Å–∫—Ä–∏–ø—Ç–∞.
--]]
local isServerDestroyerEnabled = false
local isFriendsWhitelisted = false

local function toggleServerDestroyer(enabled, whitelistFriends)
    isServerDestroyerEnabled = enabled
    isFriendsWhitelisted = whitelistFriends

    if isServerDestroyerEnabled then
        print("üß® Server Destroyer Started. Whitelist Friends: " .. tostring(isFriendsWhitelisted))

        task.spawn(function()
            local localPlayer = game.Players.LocalPlayer
            if not localPlayer then
                warn("‚ùå Local player not found.")
                isServerDestroyerEnabled = false
                return
            end

            local playersAffectedCount = 0

            while isServerDestroyerEnabled do
                local allPlayers = {}
                for _, player in ipairs(game.Players:GetPlayers()) do
                    if player ~= localPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                        if not (isFriendsWhitelisted and localPlayer:IsFriendsWith(player.UserId)) then
                            table.insert(allPlayers, player)
                        end
                    end
                end

                -- –†–∞–∑–¥–µ–ª–∏–º –Ω–∞ 2 –≥—Ä—É–ø–ø—ã
                local mid = math.ceil(#allPlayers / 2)
                local groupLeft = {}
                local groupRight = {}

                for i, p in ipairs(allPlayers) do
                    if i <= mid then
                        table.insert(groupLeft, p)
                    else
                        table.insert(groupRight, p)
                    end
                end

                -- –°–æ–±–∏—Ä–∞–µ–º –≤—Å–µ—Ö Blobman'–æ–≤
                local blobmans = {}
                for _, folder in ipairs(workspace:GetChildren()) do
                    if folder:IsA("Folder") and folder.Name:find("SpawnedInToys") then
                        for _, blob in ipairs(folder:GetChildren()) do
                            if blob:IsA("Model") and blob.Name == "CreatureBlobman" then
                                local root = blob:FindFirstChild("HumanoidRootPart")
                                if root then
                                    local seatScript = blob:FindFirstChild("BlobmanSeatAndOwnerScript")
                                    if seatScript then
                                        table.insert(blobmans, {
                                            model = blob,
                                            root = root,
                                            left = blob:FindFirstChild("LeftDetector"),
                                            right = blob:FindFirstChild("RightDetector"),
                                            leftWeld = blob:FindFirstChild("LeftDetector") and blob.LeftDetector:FindFirstChild("LeftWeld"),
                                            rightWeld = blob:FindFirstChild("RightDetector") and blob.RightDetector:FindFirstChild("RightWeld"),
                                            remote = seatScript:FindFirstChild("CreatureGrab")
                                        })
                                    end
                                end
                            end
                        end
                    end
                end

                -- –í—Å–ø–æ–º–æ–≥–∞—Ç–µ–ª—å–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è
                local function getClosestBlobman(targetPos, hand)
                    local closest, minDist = nil, math.huge
                    for _, blob in ipairs(blobmans) do
                        local weld = hand == "Left" and blob.leftWeld or blob.rightWeld
                        local detector = hand == "Left" and blob.left or blob.right
                        if blob.remote and detector and weld then
                            local dist = (blob.root.Position - targetPos).Magnitude
                            if dist < minDist then
                                closest = {remote = blob.remote, detector = detector, weld = weld}
                                minDist = dist
                            end
                        end
                    end
                    return closest
                end

                -- –ì—Ä–∞–± –ª–µ–≤–æ–π —Ä—É–∫–æ–π
                for _, player in ipairs(groupLeft) do
                    local root = player.Character:FindFirstChild("HumanoidRootPart")
                    if root then
                        local blob = getClosestBlobman(root.Position, "Left")
                        if blob then
                            local args = {blob.detector, root, blob.weld}
                            pcall(function()
                                blob.remote:FireServer(unpack(args))
                                playersAffectedCount += 1
                            end)
                            task.wait(0.05)
                        end
                    end
                end

                -- –ì—Ä–∞–± –ø—Ä–∞–≤–æ–π —Ä—É–∫–æ–π
                for _, player in ipairs(groupRight) do
                    local root = player.Character:FindFirstChild("HumanoidRootPart")
                    if root then
                        local blob = getClosestBlobman(root.Position, "Right")
                        if blob then
                            local args = {blob.detector, root, blob.weld}
                            pcall(function()
                                blob.remote:FireServer(unpack(args))
                                playersAffectedCount += 1
                            end)
                            task.wait(0.05)
                        end
                    end
                end

                task.wait(0.3)
            end

            print("üõë Server Destroyer Stopped. Players affected: " .. playersAffectedCount)
        end)
    else
        print("üîï Server Destroyer Stopping...")
    end
end


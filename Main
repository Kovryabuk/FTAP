local grabAuraEnabled = false -- –ü–µ—Ä–µ–º–µ–Ω–Ω–∞—è –¥–ª—è –æ—Ç—Å–ª–µ–∂–∏–≤–∞–Ω–∏—è —Å–æ—Å—Ç–æ—è–Ω–∏—è –∞—É—Ä—ã

-- –ü–µ—Ä–µ–º–µ–Ω–Ω–∞—è –¥–ª—è –æ—Ç—Å–ª–µ–∂–∏–≤–∞–Ω–∏—è —Å–æ—Å—Ç–æ—è–Ω–∏—è –∞—É—Ä—ã Grab Aura
local grabAuraEnabled = false
-- –ü–µ—Ä–µ–º–µ–Ω–Ω–∞—è –¥–ª—è —Ö—Ä–∞–Ω–µ–Ω–∏—è —Å—Å—ã–ª–∫–∏ –Ω–∞ –∏–Ω–¥–∏–∫–∞—Ç–æ—Ä Grab Aura
local grabAuraIndicator = nil
-- –¢–∞–±–ª–∏—Ü–∞ –¥–ª—è –æ—Ç—Å–ª–µ–∂–∏–≤–∞–Ω–∏—è –∞–∫—Ç–∏–≤–Ω—ã—Ö —Ñ—É–Ω–∫—Ü–∏–π (–¥–ª—è —Å–æ—Ö—Ä–∞–Ω–µ–Ω–∏—è/–∑–∞–≥—Ä—É–∑–∫–∏ –∏ –æ—Ç–æ–±—Ä–∞–∂–µ–Ω–∏—è)
-- (–í –≤–∞—à–µ–º –ø–æ–ª–Ω–æ–º —Å–∫—Ä–∏–ø—Ç–µ —ç—Ç–∞ —Ç–∞–±–ª–∏—Ü–∞ –¥–æ–ª–∂–Ω–∞ –±—ã—Ç—å –≥–ª–æ–±–∞–ª—å–Ω–æ–π –∏–ª–∏ –ø–µ—Ä–µ–¥–∞–≤–∞—Ç—å—Å—è)
local ActiveFunctions = ActiveFunctions or {} -- –ò—Å–ø–æ–ª—å–∑—É–µ–º —Å—É—â–µ—Å—Ç–≤—É—é—â—É—é —Ç–∞–±–ª–∏—Ü—É, –µ—Å–ª–∏ –æ–Ω–∞ –µ—Å—Ç—å

local function createGrabAura()
    if grabAuraEnabled then
        grabAuraEnabled = false
        ActiveFunctions.GrabAura = false
        print("GrabAura disabled!")
        return
    end

    grabAuraEnabled = true
    ActiveFunctions.GrabAura = true
    print("GrabAura enabled!")

    task.spawn(function()
        while grabAuraEnabled do
            local localPlayer = game:GetService("Players").LocalPlayer
            local character = localPlayer.Character

            if character and character:FindFirstChild("HumanoidRootPart") then
                local closestPlayer = nil
                local closestDistance = 20

                for _, player in pairs(game:GetService("Players"):GetPlayers()) do
                    if player ~= localPlayer and player.Character and player.Character:FindFirstChild("Head") then
                        local distance = (character.HumanoidRootPart.Position - player.Character.Head.Position).Magnitude

                        -- –ü—Ä–æ–≤–µ—Ä–∫–∞: –Ω–µ –¥—Ä—É–≥, –≤ –∑–æ–Ω–µ —Ä–∞–¥–∏—É—Å–∞
                        if distance <= closestDistance and not localPlayer:IsFriendsWith(player.UserId) then
                            -- –ü–æ—Å—Ç–æ—è–Ω–Ω–∞—è –ø—Ä–æ–≤–µ—Ä–∫–∞: –µ—Å—Ç—å –ª–∏ IsHeld –∏ –µ–≥–æ –∑–Ω–∞—á–µ–Ω–∏–µ false
                            local isHeld = player.Character:FindFirstChild("IsHeld")
                            if not isHeld or isHeld.Value == false then  -- –ï—Å–ª–∏ IsHeld –æ—Ç—Å—É—Ç—Å—Ç–≤—É–µ—Ç –∏–ª–∏ –µ–≥–æ –∑–Ω–∞—á–µ–Ω–∏–µ false
                                closestPlayer = player
                                closestDistance = distance
                            end
                        end
                    end
                end

                -- –ï—Å–ª–∏ –Ω–∞–π–¥–µ–Ω –±–ª–∏–∂–∞–π—à–∏–π –∏–≥—Ä–æ–∫, –≤—ã–ø–æ–ª–Ω—è–µ–º –Ω—É–∂–Ω—É—é –æ–ø–µ—Ä–∞—Ü–∏—é
                if closestPlayer then
                    local args = {
                        [1] = closestPlayer.Character.Head,
                        [2] = closestPlayer.Character.Head.CFrame
                    }

                    local grabEvents = game:GetService("ReplicatedStorage"):WaitForChild("GrabEvents", 10)
                    if grabEvents then
                        local setNetworkOwnerRemote = grabEvents:WaitForChild("SetNetworkOwner", 10)
                        if setNetworkOwnerRemote then
                            pcall(function()
                                setNetworkOwnerRemote:FireServer(unpack(args))
                            end)
                        end
                    end
                end
            end

            task.wait(0.1) -- –ù–µ–±–æ–ª—å—à–∞—è –ø–∞—É–∑–∞ –¥–ª—è —Å–Ω–∏–∂–µ–Ω–∏—è –Ω–∞–≥—Ä—É–∑–∫–∏
        end
    end)
end

local function setupGrabAura()
    local grabAuraToggle = createToggle(auraTab, "Grab Aura", 1, function()
        createGrabAura()
    end)
end

local function setupBringTeleport()
    local bringLabel = createLabel(teleportTab, "Bring Player", 3)
    local playerDropdown = createPlayerDropdown(teleportTab, 4)
    local bringButton = createButton(teleportTab, "Bring Selected Player", 5, function()
        local targetName = playerDropdown.getSelectedPlayer()
        if not targetName then
            displayMessage("Select a player first!", 2)
            return
        end

        local Players = game:GetService("Players")
        local ReplicatedStorage = game:GetService("ReplicatedStorage")
        local Lighting = game:GetService("Lighting")
        local TweenService = game:GetService("TweenService")

        local localPlayer = Players.LocalPlayer
        local character = localPlayer.Character or localPlayer.CharacterAdded:Wait()
        local root = character:WaitForChild("HumanoidRootPart")
        local oldCFrame = root.CFrame

        local targetPlayer = Players:FindFirstChild(targetName)
        if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("Head") then
            local targetHead = targetPlayer.Character.Head

            -- –ü–æ—Å—Ç–æ—è–Ω–Ω—ã–π —Ç–µ–ª–µ–ø–æ—Ä—Ç –∫ —Ü–µ–ª–∏ + FireServer –∫–∞–∂–¥—ã–π —Ä–∞–∑
            for i = 1, 2 do -- 50 –∏—Ç–µ—Ä–∞—Ü–∏–π ‚âà 7.5 —Å–µ–∫
                if not targetPlayer.Character or not targetPlayer.Character:FindFirstChild("Head") then break end

                -- –¢–ü —Å–µ–±—è –∫ —Ü–µ–ª–∏
                root.CFrame = targetHead.CFrame * CFrame.new(2, 0, 0)

                -- FireServer –∫–∞–∂–¥—ã–π —Ä–∞–∑
                local args = { [1] = targetHead, [2] = root.CFrame }
                ReplicatedStorage:WaitForChild("GrabEvents"):WaitForChild("SetNetworkOwner"):FireServer(unpack(args))

                task.wait(0.15)
            end

            task.wait(0.1)

            -- –í –∫–æ–Ω—Ü–µ –ø–µ—Ä–µ–Ω–æ—Å
            root.CFrame = oldCFrame
            local front = oldCFrame.LookVector * 5
            targetHead.CFrame = CFrame.new(oldCFrame.Position + front)

            -- DestroyGrabLine
            local destroyArgs = { [1] = targetHead }
            ReplicatedStorage:WaitForChild("GrabEvents"):WaitForChild("DestroyGrabLine"):FireServer(unpack(destroyArgs))
        else
            displayMessage("Player not valid", 2)
        end
    end)
end

local function setupTeleportToPlayer()
    local teleportLabel = createLabel(teleportTab, "Teleport to Player", 6)
    local playerDropdown = createPlayerDropdown(teleportTab, 7)

    local teleportButton = createButton(teleportTab, "Teleport", 8, function()
        local targetName = playerDropdown.getSelectedPlayer()
        if not targetName then
            displayMessage("Select a player first!", 2)
            return
        end

        local Players = game:GetService("Players")
        local localPlayer = Players.LocalPlayer
        local character = localPlayer.Character or localPlayer.CharacterAdded:Wait()
        local root = character:FindFirstChild("HumanoidRootPart")

        local targetPlayer = Players:FindFirstChild(targetName)
        if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
            local targetRoot = targetPlayer.Character.HumanoidRootPart
            root.CFrame = targetRoot.CFrame + Vector3.new(2, 0, 0) -- —á—É—Ç—å —Å–±–æ–∫—É –æ—Ç –∏–≥—Ä–æ–∫–∞
            displayMessage("Teleported to " .. targetName, 2)
        else
            displayMessage("Player not valid", 2)
        end
    end)
end

    -- –í—ã–∑—ã–≤–∞–µ–º –≤—Å–µ —Ñ—É–Ω–∫—Ü–∏–∏ –Ω–∞—Å—Ç—Ä–æ–π–∫–∏
    setupAntiGrab()
    setupAntiExplosion() -- –î–æ–±–∞–≤–ª—è–µ–º –≤—ã–∑–æ–≤ —Ñ—É–Ω–∫—Ü–∏–∏ –Ω–∞—Å—Ç—Ä–æ–π–∫–∏ –ê–Ω—Ç–∏-–≤–∑—Ä—ã–≤–æ–≤
    setupPlayerSpeed()
    setupInfiniteJump()
    setupNoClip()
    setupMouseTeleportKeybind()
    setupPalletKeybind()
    setupAnchorKeybind()
    setupWaterCollision()
    setupAntiBarrier()
    setupSuperStrength()
    setupGrabAura()
    setupBringTeleport()
    setupAntiBlobman()
    setupTeleportToPlayer()
    setupKeyBindings()


    -- –£—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –ø–µ—Ä–≤—É—é –≤–∫–ª–∞–¥–∫—É –∫–∞–∫ –∞–∫—Ç–∏–≤–Ω—É—é –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é
    for _, btn in pairs(Tabs:GetChildren()) do
        if btn:IsA("TextButton") then
            btn.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
            if btn.Text == "Main" then
                btn.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
            end
        end
    end

    for _, v in pairs(Content:GetChildren()) do
        if v:IsA("ScrollingFrame") then
            v.Visible = false
            if v.Name == "MainTab" then
                v.Visible = true
            end
        end
    end

    return ScreenGui
end


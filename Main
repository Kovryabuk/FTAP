    local bringBlobmanButton = createButton(blobmanTab, "Bring Blobman", 4, function()
        local spawningPlayerName = game.Players.LocalPlayer.Name
        local targetPlayerName = playerDropdown.getSelectedPlayer() -- –ü–æ–ª—É—á–∞–µ–º –≤—ã–±—Ä–∞–Ω–Ω–æ–≥–æ –∏–≥—Ä–æ–∫–∞ –∏–∑ –≤—ã–ø–∞–¥–∞—é—â–µ–≥–æ —Å–ø–∏—Å–∫–∞

        if not targetPlayerName then
            print("Please select a target player.")
            return
        end

        -- –î–∏–Ω–∞–º–∏—á–µ—Å–∫–∏ —Å–æ–∑–¥–∞–µ–º –∏ –≤—ã–ø–æ–ª–Ω—è–µ–º –∫–æ–¥
        local codeToExecute = string.format([[
            local spawningPlayerName = "%s"
            local targetPlayerName = "%s"

            local targetPlayer = game:GetService("Players"):FindFirstChild(targetPlayerName)
            if not targetPlayer or not targetPlayer.Character or not targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
                warn("Target player or their character not found.")
                return
            end

            local blobmanCreature = workspace:FindFirstChild(spawningPlayerName .. "SpawnedInToys")
            if not blobmanCreature or not blobmanCreature:FindFirstChild("CreatureBlobman") then
                 warn("Blobman creature not found for local player.")
                 return
            end

            local leftDetector = blobmanCreature.CreatureBlobman:FindFirstChild("LeftDetector")
            local leftWeld = leftDetector and leftDetector:FindFirstChild("LeftWeld")

            if not leftDetector or not leftWeld then
                 warn("Blobman parts (LeftDetector or LeftWeld) not found.")
                 return
            end


            local args = {
                [1] = leftDetector,
                [2] = targetPlayer.Character.HumanoidRootPart,
                [3] = leftWeld
            }

            local remoteFunction = blobmanCreature.CreatureBlobman:FindFirstChild("BlobmanSeatAndOwnerScript")
            if remoteFunction and remoteFunction:FindFirstChild("CreatureGrab") then
                 remoteFunction.CreatureGrab:FireServer(unpack(args))
                 print("Attempted to bring Blobman to " .. targetPlayerName)
            else
                 warn("CreatureGrab RemoteFunction not found.")
            end

        ]], spawningPlayerName, targetPlayerName)

        local success, err = pcall(loadstring(codeToExecute))
        if not success then
            warn("Error executing Blobman bring script: " .. err)
        end
    end)

--[[
  –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è —è–∫–æ—Ä—è (anchor) –Ω–∞ –æ—Å–Ω–æ–≤–µ GrabParts.
  –û–Ω–∞ –∏—â–µ—Ç –æ—Ä–∏–≥–∏–Ω–∞–ª—å–Ω—ã–π –æ–±—ä–µ–∫—Ç "GrabParts", –æ–ø—Ä–µ–¥–µ–ª—è–µ—Ç, —á–µ–≥–æ –æ–Ω –∫–∞—Å–∞–µ—Ç—Å—è,
  –∫–ª–æ–Ω–∏—Ä—É–µ—Ç –µ–≥–æ, –æ—á–∏—â–∞–µ—Ç –æ—Ç –Ω–µ–∫–æ—Ç–æ—Ä—ã—Ö —ç–ª–µ–º–µ–Ω—Ç–æ–≤ –∏ –ø–æ–∑–∏—Ü–∏–æ–Ω–∏—Ä—É–µ—Ç –∫–ª–æ–Ω
  –Ω–∞ –º–µ—Å—Ç–µ –ø–µ—Ä–≤–æ–≥–æ –∫–∞—Å–∞–Ω–∏—è, –¥–æ–±–∞–≤–ª—è—è –≤–∏–∑—É–∞–ª—å–Ω—ã–π Highlight.
--]]
local function createAnchor()
    -- –ü—Ä–æ–≤–µ—Ä—è–µ–º, —Å—É—â–µ—Å—Ç–≤—É–µ—Ç –ª–∏ —É–∂–µ –æ—á–∏—â–µ–Ω–Ω–∞—è –º–æ–¥–µ–ª—å —è–∫–æ—Ä—è, –∏ —É–¥–∞–ª—è–µ–º –µ–µ, –µ—Å–ª–∏ –¥–∞
    local existing = workspace:FindFirstChild("CleanedGrabParts")
    if existing then
        existing:Destroy()
        return nil
    end
    
    -- –ò—â–µ–º –æ—Ä–∏–≥–∏–Ω–∞–ª—å–Ω—É—é –º–æ–¥–µ–ª—å GrabParts –≤ Workspace
    local originalModel = workspace:FindFirstChild("GrabParts")
    
    -- –ï—Å–ª–∏ –æ—Ä–∏–≥–∏–Ω–∞–ª—å–Ω–∞—è –º–æ–¥–µ–ª—å –Ω–µ –Ω–∞–π–¥–µ–Ω–∞, –≤—ã–≤–æ–¥–∏–º —Å–æ–æ–±—â–µ–Ω–∏–µ –∏ –≤—ã—Ö–æ–¥–∏–º –∏–∑ —Ñ—É–Ω–∫—Ü–∏–∏
    if not originalModel then
        warn("GrabParts model not found in workspace.")
        return nil
    end
    
    -- –í–∫–ª—é—á–∞–µ–º –∫–æ–ª–ª–∏–∑–∏—é –¥–ª—è –ø—Ä–æ–≤–µ—Ä–∫–∏ –∫–∞—Å–∞–Ω–∏–π
    local parts = {}
    for _, part in pairs(originalModel:GetDescendants()) do
        if part:IsA("BasePart") then
            -- –ó–∞–ø–æ–º–∏–Ω–∞–µ–º –∏—Å—Ö–æ–¥–Ω–æ–µ —Å–æ—Å—Ç–æ—è–Ω–∏–µ CanCollide
            local originalCanCollide = part.CanCollide
            part.CanCollide = true
            table.insert(parts, {part = part, originalCanCollide = originalCanCollide})
        end
    end
    
    -- –ñ–¥—ë–º –∫–∞–¥—Ä, —á—Ç–æ–±—ã –∫–æ–ª–ª–∏–∑–∏–∏ –æ–±–Ω–æ–≤–∏–ª–∏—Å—å
    task.wait(0.1)
    
    -- –ü—Ä–æ–≤–µ—Ä—è–µ–º, –µ—Å—Ç—å –ª–∏ –∫–∞—Å–∞—é—â–∏–µ—Å—è —á–∞—Å—Ç–∏
    local hasTouching = false
    for _, partData in pairs(parts) do
        local touching = partData.part:GetTouchingParts()
        for _, hit in pairs(touching) do
            if not hit:IsDescendantOf(originalModel) then
                hasTouching = true
                break
            end
        end
        if hasTouching then
            break
        end
    end
    
    -- –í–æ–∑–≤—Ä–∞—â–∞–µ–º –æ—Ä–∏–≥–∏–Ω–∞–ª—å–Ω–æ–µ —Å–æ—Å—Ç–æ—è–Ω–∏–µ CanCollide
    for _, partData in pairs(parts) do
        partData.part.CanCollide = partData.originalCanCollide
    end
    
    -- –ï—Å–ª–∏ –Ω–µ—Ç –∫–∞—Å–∞—é—â–∏—Ö—Å—è —á–∞—Å—Ç–µ–π, –≤—ã–≤–æ–¥–∏–º —Å–æ–æ–±—â–µ–Ω–∏–µ –∏ –≤—ã—Ö–æ–¥–∏–º
    if not hasTouching then
        warn("No touching parts found for GrabParts.")
        return nil
    end
    
    -- –ö–ª–æ–Ω–∏—Ä—É–µ–º –º–æ–¥–µ–ª—å 
    local cleanedModel = originalModel:Clone()
    cleanedModel.Name = "CleanedGrabParts"
    
    -- –û—á–∏—â–∞–µ–º –∫–ª–æ–Ω–∏—Ä–æ–≤–∞–Ω–Ω—É—é –º–æ–¥–µ–ª—å (—Ç–æ–ª—å–∫–æ —É–¥–∞–ª—è–µ–º –∑–≤—É–∫–∏ –∏ –ª—É—á–∏)
    for _, v in pairs(cleanedModel:GetDescendants()) do
        if v:IsA("BasePart") and (v.Name == "GrabPart" or v.Name == "DragPart") then
            -- –£–¥–∞–ª—è–µ–º –∑–≤—É–∫–∏
            for _, soundName in pairs({"AttachSound1", "AttachSound", "BeamSound", "BeamSound1"}) do
                local sound = v:FindFirstChild(soundName)
                if sound then
                    sound:Destroy()
                end
            end
            -- –î–µ–ª–∞–µ–º —á–∞—Å—Ç—å –Ω–µ–≤–∏–¥–∏–º–æ–π
            v.Transparency = 1
        elseif v:IsA("BasePart") and v.Name == "BeamPart" then
            -- –£–¥–∞–ª—è–µ–º –ª—É—á
            local beam = v:FindFirstChild("GrabBeam")
            if beam then
                beam:Destroy()
            end
        end
    end
    
    -- –°–æ–∑–¥–∞–µ–º –≤–∏–∑—É–∞–ª—å–Ω—ã–π –∏–Ω–¥–∏–∫–∞—Ç–æ—Ä
    local indicator = Instance.new("Part")
    indicator.Name = "AnchorIndicator"
    indicator.Size = Vector3.new(0.5, 0.5, 0.5)
    indicator.Anchored = true
    indicator.CanCollide = false
    indicator.Material = Enum.Material.Neon
    indicator.BrickColor = BrickColor.new("Navy blue")
    indicator.Transparency = 0.3
    indicator.Shape = Enum.PartType.Ball
    indicator.CFrame = originalModel:GetPivot()
    indicator.Parent = cleanedModel
    
    -- –£—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º —Ä–æ–¥–∏—Ç–µ–ª—è –º–æ–¥–µ–ª–∏ –Ω–∞ workspace
    cleanedModel.Parent = workspace
    
    print("Anchor created successfully!")
    
    return cleanedModel
end

    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è –ø–µ—Ä–µ–∫–ª—é—á–∞—Ç–µ–ª—è –∫–æ–ª–ª–∏–∑–∏–∏ –≤–æ–¥—ã
    local function setupWaterCollision()
        -- –°–æ–∑–¥–∞–µ–º –ø–µ—Ä–µ–∫–ª—é—á–∞—Ç–µ–ª—å –¥–ª—è –∫–æ–ª–ª–∏–∑–∏–∏ –≤–æ–¥—ã
        local waterCollisionToggle = createToggle(otherTab, "Water Collision", 2, function(enabled)
            if enabled then
                -- –í–∫–ª—é—á–µ–Ω–∏–µ –∫–æ–ª–ª–∏–∑–∏–∏
                local parentModel = workspace:FindFirstChild("Map")
                    and workspace.Map:FindFirstChild("AlwaysHereTweenedObjects")
                    and workspace.Map.AlwaysHereTweenedObjects:FindFirstChild("Ocean")
                    and workspace.Map.AlwaysHereTweenedObjects.Ocean:FindFirstChild("Object")
                    and workspace.Map.AlwaysHereTweenedObjects.Ocean.Object:FindFirstChild("ObjectModel")
                if parentModel then
                    for _, child in ipairs(parentModel:GetChildren()) do
                        if child.Name == "Ocean" and child:IsA("BasePart") then
                            child.CanCollide = true  -- –í–∫–ª—é—á–∞–µ–º –∫–æ–ª–ª–∏–∑–∏—é
                        end
                    end
                end
            else
                -- –í—ã–∫–ª—é—á–µ–Ω–∏–µ –∫–æ–ª–ª–∏–∑–∏–∏
                local parentModel = workspace:FindFirstChild("Map")
                    and workspace.Map:FindFirstChild("AlwaysHereTweenedObjects")
                    and workspace.Map.AlwaysHereTweenedObjects:FindFirstChild("Ocean")
                    and workspace.Map.AlwaysHereTweenedObjects.Ocean:FindFirstChild("Object")
                    and workspace.Map.AlwaysHereTweenedObjects.Ocean.Object:FindFirstChild("ObjectModel")
                if parentModel then
                    for _, child in ipairs(parentModel:GetChildren()) do
                        if child.Name == "Ocean" and child:IsA("BasePart") then
                            child.CanCollide = false  -- –í—ã–∫–ª—é—á–∞–µ–º –∫–æ–ª–ª–∏–∑–∏—é
                        end
                    end
                end
            end
        end)
    end

    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –¥–æ–±–∞–≤–ª–µ–Ω–∏—è AntiGrab –∏–∑ FTAP API
    local function setupAntiGrab()
        -- –°–æ–∑–¥–∞–µ–º –ø–µ—Ä–µ–∫–ª—é—á–∞—Ç–µ–ª—å –¥–ª—è –≤–∫–ª—é—á–µ–Ω–∏—è AntiGrab
        local antiGrabToggle = createToggle(defenceTab, "Anti Grab", 1, function(enabled)
            if enabled then
                ActiveFunctions.AntiGrab = true
                ApiFTAP.AntiGrab(true)
            else
                ActiveFunctions.AntiGrab = false
                ApiFTAP.AntiGrab(false)
            end
        end)
    end

    local function setupAntiBarrier()
        local toggle = createToggle(otherTab, "Anti Barrier", 3, function(enabled)
            local plots = workspace:FindFirstChild("Plots")
            if not plots then return end
            for _, plot in ipairs(plots:GetChildren()) do
                local barrierModel = plot:FindFirstChild("Barrier")
                if barrierModel then
                    for _, part in ipairs(barrierModel:GetChildren()) do
                        if part:IsA("BasePart") and part.Name == "PlotBarrier" then
                            part.CanCollide = not enabled
                        end
                    end
                end
            end
        end)
    end

    local function setupPlayerSpeed()
        local speedSlider = createSlider(playerTab, "Custom Velocity", 0, 10000, 0, 2, function(value)
            local player = game.Players.LocalPlayer
            local character = player.Character or player.CharacterAdded:Wait()
            local root = character:FindFirstChild("HumanoidRootPart")
            -- –£–¥–∞–ª—è–µ–º —Å—Ç–∞—Ä—ã–π BodyVelocity
            if root then
                local existing = root:FindFirstChild("SpeedVelocity")
                if existing then existing:Destroy() end
                -- –ï—Å–ª–∏ –∑–Ω–∞—á–µ–Ω–∏–µ –±–æ–ª—å—à–µ 0, —Å–æ–∑–¥–∞—ë–º –Ω–æ–≤—ã–π
                if value > 0 then
                    local bv = Instance.new("BodyVelocity")
                    bv.Name = "SpeedVelocity"
                    bv.Velocity = character.Humanoid.MoveDirection * value
                    bv.MaxForce = Vector3.new(1e5, 0, 1e5)
                    bv.Parent = root
                    -- –û–±–Ω–æ–≤–ª—è–µ–º –∫–∞–∂–¥—ã–π –∫–∞–¥—Ä –≤ —Å–æ–æ—Ç–≤–µ—Ç—Å—Ç–≤–∏–∏ —Å –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–µ–º
                    game:GetService("RunService").RenderStepped:Connect(function()
                        if bv and bv.Parent then
                            bv.Velocity = character.Humanoid.MoveDirection * value
                        end
                    end)
                end
            end
        end)
        -- Toggle –¥–ª—è –ø–æ—Å—Ç–æ—è–Ω–Ω–æ–π —Å–∫–æ—Ä–æ—Å—Ç–∏ –Ω–∞ —Ä–µ—Å–ø–∞–≤–Ω–µ
        local keepVelocityToggle = createToggle(playerTab, "Keep Velocity On Respawn", 4, function(enabled)
            ActiveFunctions.KeepVelocity = enabled
            if enabled then
                local savedValue = speedSlider.getValue()
                game.Players.LocalPlayer.CharacterAdded:Connect(function(char)
                    local root = char:WaitForChild("HumanoidRootPart")
                    task.wait(0.1)
                    local bv = Instance.new("BodyVelocity")
                    bv.Name = "SpeedVelocity"
                    bv.Velocity = char.Humanoid.MoveDirection * savedValue
                    bv.MaxForce = Vector3.new(1e5, 0, 1e5)
                    bv.Parent = root
                    game:GetService("RunService").RenderStepped:Connect(function()
                        if bv and bv.Parent then
                            bv.Velocity = char.Humanoid.MoveDirection * savedValue
                        end
                    end)
                end)
            end
        end)
    end

local UserInputService = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")

local isZoomed = false
local defaultFOV = 70
local zoomedFOV = 10

-- –§—É–Ω–∫—Ü–∏—è –ø–µ—Ä–µ–∫–ª—é—á–µ–Ω–∏—è –∑—É–º–∞
local function toggleZoom()
    local camera = workspace.CurrentCamera
    if not camera then return end

    isZoomed = not isZoomed

    local targetFOV = isZoomed and zoomedFOV or defaultFOV

    TweenService:Create(
        camera,
        TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
        {FieldOfView = targetFOV}
    ):Play()

    print("Zoom " .. (isZoomed and "ENABLED" or "DISABLED"))
end

-- –û–±—Ä–∞–±–æ—Ç–∫–∞ –Ω–∞–∂–∞—Ç–∏–π
UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    if input.KeyCode == Enum.KeyCode.Z then
        toggleZoom()
    end
end)


    local function setupInfiniteJump()
        local jumpPower = 50
        local activeBV = nil
        -- –ü–µ—Ä–µ–∫–ª—é—á–∞—Ç–µ–ª—å
        local infiniteJumpToggle = createToggle(playerTab, "Infinite Jump", 5, function(enabled)
            ActiveFunctions.InfiniteJump = enabled
        end)
        -- –°–ª–∞–π–¥–µ—Ä –¥–ª—è —Å–∏–ª—ã
        local jumpPowerSlider = createSlider(playerTab, "Jump Power", 50, 200, 50, 7, function(value)
            jumpPower = value
        end)
        -- –û–±—Ä–∞–±–æ—Ç–∫–∞ –∑–∞–∂–∞—Ç–∏—è/–æ—Ç–ø—É—Å–∫–∞–Ω–∏—è –ø—Ä–æ–±–µ–ª–∞
        local UIS = game:GetService("UserInputService")
        UIS.InputBegan:Connect(function(input, processed)
            if processed or not ActiveFunctions.InfiniteJump then return end
            if input.KeyCode == Enum.KeyCode.Space then
                local char = game.Players.LocalPlayer.Character
                if not char then return end
                local root = char:FindFirstChild("HumanoidRootPart")
                if not root then return end
                -- –°–æ–∑–¥–∞–µ–º BodyVelocity
                local bv = Instance.new("BodyVelocity")
                bv.Name = "InfiniteJumpVelocity"
                bv.Velocity = Vector3.new(0, jumpPower, 0)
                bv.MaxForce = Vector3.new(0, 1e5, 0)
                bv.Parent = root
                activeBV = bv
            end
        end)
        UIS.InputEnded:Connect(function(input)
            if input.KeyCode == Enum.KeyCode.Space and activeBV then
                activeBV:Destroy()
                activeBV = nil
            end
        end)
    end

    -- –§—É–Ω–∫—Ü–∏—è —Å–ø–∞–≤–Ω–∞ –ø–∞–ª–ª–µ—Ç–∞ –ø–µ—Ä–µ–¥ –∏–≥—Ä–æ–∫–æ–º
    local function spawnPallet()
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

        -- –ü–æ–ª—É—á–∞–µ–º —Ç–µ–∫—É—â—É—é –ø–æ–∑–∏—Ü–∏—é –∏ –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
        local currentPosition = humanoidRootPart.Position
        local lookVector = humanoidRootPart.CFrame.LookVector

        -- –°–æ–∑–¥–∞–µ–º –ø–æ–∑–∏—Ü–∏—é –ø–µ—Ä–µ–¥ –∏–≥—Ä–æ–∫–æ–º (2 –µ–¥–∏–Ω–∏—Ü—ã –≤–ø–µ—Ä–µ–¥ –≤–º–µ—Å—Ç–æ 5)
        local targetPosition = currentPosition + (lookVector * 1)

        -- –°–æ–∑–¥–∞–µ–º CFrame —Å–º–æ—Ç—Ä—è—â–∏–π –≤ —Ç—É –∂–µ —Å—Ç–æ—Ä–æ–Ω—É, —á—Ç–æ –∏ –∏–≥—Ä–æ–∫
        local targetCFrame = CFrame.new(targetPosition, targetPosition + lookVector)

        -- –î–æ–±–∞–≤–ª—è–µ–º –Ω–µ–±–æ–ª—å—à–æ–π –æ—Ç—Å—Ç—É–ø –≤–≤–µ—Ä—Ö, —á—Ç–æ–±—ã –ø–∞–ª–ª–µ—Ç –Ω–µ –∑–∞—Å—Ç—Ä—è–ª –≤ –∑–µ–º–ª–µ
        targetCFrame = targetCFrame * CFrame.new(0, 0.5, 0)

        -- –í—ã–∑—ã–≤–∞–µ–º —Ñ—É–Ω–∫—Ü–∏—é —Å–ø–∞–≤–Ω–∞ –ø–∞–ª–ª–µ—Ç–∞
        pcall(function()
            local args = {
                [1] = "PalletLightBrown",
                [2] = targetCFrame,
                [3] = Vector3.new(0, 31.239999771118164, 0)
            }
            game:GetService("ReplicatedStorage"):WaitForChild("MenuToys"):WaitForChild("SpawnToyRemoteFunction"):InvokeServer(unpack(args))
        end)
    end

    -- –î–æ–±–∞–≤–ª—è–µ–º —Ñ—É–Ω–∫—Ü–∏—é —Å–ø–∞–≤–Ω–∞ –ø–∞–ª–ª–µ—Ç–∞ –Ω–∞ –∫–ª–∞–≤–∏—à—É Tab
    local function setupPalletKeybind()
        -- –°–æ–∑–¥–∞–µ–º –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—é –æ –∫–ª–∞–≤–∏—à–µ
        local palletKeybindInfo = createKeybindInfo(keybindsTab, "Spawn Pallet", "Tab", 2)

        -- –°–æ–∑–¥–∞–µ–º –ø–µ—Ä–µ–∫–ª—é—á–∞—Ç–µ–ª—å –¥–ª—è –∞–∫—Ç–∏–≤–∞—Ü–∏–∏/–¥–µ–∞–∫—Ç–∏–≤–∞—Ü–∏–∏ –∫–ª–∞–≤–∏—à–∏
        local palletKeybindToggle = createToggle(keybindsTab, "Enable Pallet Keybind", 4, function(enabled)
            ActiveKeybinds.PalletSpawn = enabled
        end)
    end

    -- –§—É–Ω–∫—Ü–∏—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞ –ø–æ –º—ã—à–∫–µ —Å –æ–≥—Ä–∞–Ω–∏—á–µ–Ω–∏–µ–º –ø–æ —Ä–∞—Å—Å—Ç–æ—è–Ω–∏—é
    local function teleportToMouse()
        local player = game.Players.LocalPlayer
        local mouse = player:GetMouse()
        local character = player.Character
        local humanoid = character and character:FindFirstChild("Humanoid")
        local root = character and character:FindFirstChild("HumanoidRootPart")

        if not (root and humanoid and humanoid.Health > 0) then return end
        local targetPosition = mouse.Hit.Position
        local currentPosition = root.Position
        local maxDistance = 2000
        if mouse.Target and (targetPosition - currentPosition).Magnitude <= maxDistance then
            root.CFrame = CFrame.new(targetPosition + Vector3.new(0, 3, 0))
        end
    end

    -- –£—Å—Ç–∞–Ω–æ–≤–∫–∞ –∫–ª–∞–≤–∏—à–∏ T –¥–ª—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞ –∏ –ø–µ—Ä–µ–∫–ª—é—á–∞—Ç–µ–ª—è
    local function setupMouseTeleportKeybind()
        -- –ò–Ω—Ñ–æ –æ –∫–ª–∞–≤–∏—à–µ
        local keybindInfo = createKeybindInfo(teleportTab, "Click Teleport", "T", 2)
        -- –ü–µ—Ä–µ–∫–ª—é—á–∞—Ç–µ–ª—å
        local teleportToggle = createToggle(teleportTab, "Enable Click Teleport", 3, function(enabled)
            ActiveKeybinds.ClickTeleport = enabled
        end)
        -- –û–±—Ä–∞–±–æ—Ç–∫–∞ –∫–ª–∞–≤–∏—à–∏ T
        game:GetService("UserInputService").InputBegan:Connect(function(input, gp)
            if gp then return end
            if input.KeyCode == Enum.KeyCode.T and ActiveKeybinds.ClickTeleport then
                teleportToMouse()
            end
        end)
    end

    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –¥–æ–±–∞–≤–ª–µ–Ω–∏—è NoClip (–ø—Ä–æ—Ö–æ–∂–¥–µ–Ω–∏–µ —Å–∫–≤–æ–∑—å —Å—Ç–µ–Ω—ã)
    local function setupNoClip()
        -- –°–æ–∑–¥–∞–µ–º –ø–µ—Ä–µ–∫–ª—é—á–∞—Ç–µ–ª—å –¥–ª—è –≤–∫–ª—é—á–µ–Ω–∏—è NoClip
        local noClipToggle = createToggle(playerTab, "No Clip", 9, function(enabled)
            if enabled then
                ActiveFunctions.NoClip = true

                -- –ó–∞–ø—É—Å–∫–∞–µ–º –ø–µ—Ç–ª—é –¥–ª—è NoClip
                spawn(function()
                    while ActiveFunctions.NoClip do
                        local player = game.Players.LocalPlayer
                        local character = player.Character

                        if character then
                            for _, part in pairs(character:GetDescendants()) do
                                if part:IsA("BasePart") then
                                    part.CanCollide = false
                                end
                            end
                        end

                        task.wait(0.1) -- –ü—Ä–æ–≤–µ—Ä—è–µ–º –∫–∞–∂–¥—É—é 1/10 —Å–µ–∫—É–Ω–¥—ã
                    end
                end)
            else
                ActiveFunctions.NoClip = false

                -- –í–æ—Å—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –æ–±—ã—á–Ω—É—é –∫–æ–ª–ª–∏–∑–∏—é
                local player = game.Players.LocalPlayer
                local character = player.Character

                if character then
                    for _, part in pairs(character:GetDescendants()) do
                        if part:IsA("BasePart") and part.Name ~= "HumanoidRootPart" then
                            part.CanCollide = true
                        end
                    end
                end
            end
        end)
    end

    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –¥–æ–±–∞–≤–ª–µ–Ω–∏—è Anti-Explosion
    local function setupAntiExplosion()
        local lastGoodPosition = nil
        local stateChangedConnection = nil

        local function handleStateChanged(oldState, newState)
            local player = game.Players.LocalPlayer
            local character = player and player.Character
            local root = character and character:FindFirstChild("HumanoidRootPart")

            if not root then return end

            -- –û–±–Ω–æ–≤–ª—è–µ–º –ø–æ—Å–ª–µ–¥–Ω—é—é "—Ö–æ—Ä–æ—à—É—é" –ø–æ–∑–∏—Ü–∏—é, –∫–æ–≥–¥–∞ –∏–≥—Ä–æ–∫ –Ω–µ –Ω–∞—Ö–æ–¥–∏—Ç—Å—è –≤ —Å–æ—Å—Ç–æ—è–Ω–∏–∏, –≤—ã–∑–≤–∞–Ω–Ω–æ–º –≤–∑—Ä—ã–≤–æ–º
            if newState == Enum.HumanoidStateType.Running or
               newState == Enum.HumanoidStateType.Idle or
               newState == Enum.HumanoidStateType.Jumping or
               newState == Enum.HumanoidStateType.Climbing or
               newState == Enum.HumanoidStateType.Swimming or
               newState == Enum.HumanoidStateType.Landed then
                lastGoodPosition = root.Position
            end

            -- –ï—Å–ª–∏ —Å–æ—Å—Ç–æ—è–Ω–∏–µ –º–µ–Ω—è–µ—Ç—Å—è –Ω–∞ Falling –∏–ª–∏ Physics (—á–∞—Å—Ç–æ –ø–æ—Å–ª–µ –≤–∑—Ä—ã–≤–∞)
            if newState == Enum.HumanoidStateType.Falling or
               newState == Enum.HumanoidStateType.Physics then
                if lastGoodPosition then
                    -- –ü—ã—Ç–∞–µ–º—Å—è –≤–µ—Ä–Ω—É—Ç—å –∏–≥—Ä–æ–∫–∞ –Ω–∞ –ø–æ—Å–ª–µ–¥–Ω—é—é —Ö–æ—Ä–æ—à—É—é –ø–æ–∑–∏—Ü–∏—é
                    root.CFrame = CFrame.new(lastGoodPosition)
                    -- –ú–æ–∂–Ω–æ –≤—Ä–µ–º–µ–Ω–Ω–æ –∑–∞—Ñ–∏–∫—Å–∏—Ä–æ–≤–∞—Ç—å –∏–≥—Ä–æ–∫–∞ –Ω–∞ –º–µ—Å—Ç–µ, —á—Ç–æ–±—ã –ø—Ä–µ–¥–æ—Ç–≤—Ä–∞—Ç–∏—Ç—å –Ω–µ–º–µ–¥–ª–µ–Ω–Ω–æ–µ –æ—Ç–±—Ä–∞—Å—ã–≤–∞–Ω–∏–µ
                    -- –≠—Ç–æ –º–æ–∂–µ—Ç –±—ã—Ç—å —Å–ø–æ—Ä–Ω–æ, –∑–∞–≤–∏—Å–∏—Ç –æ—Ç –∏–≥—Ä—ã, –Ω–æ –º–æ–∂–µ—Ç –ø–æ–º–æ—á—å
                    -- local originalAnchored = root.Anchored
                    -- root.Anchored = true
                    -- task.wait(0.1) -- –ö–æ—Ä–æ—Ç–∫–∞—è —Ñ–∏–∫—Å–∞—Ü–∏—è
                    -- root.Anchored = originalAnchored
                end
            end
        end

        -- –°–æ–∑–¥–∞–µ–º –ø–µ—Ä–µ–∫–ª—é—á–∞—Ç–µ–ª—å –¥–ª—è –≤–∫–ª—é—á–µ–Ω–∏—è Anti-Explosion
        local antiExplosionToggle = createToggle(defenceTab, "Anti Explosion", 3, function(enabled) -- –ü–æ—Ä—è–¥–æ–∫ layoutOrder –º–æ–∂–µ—Ç –ø–æ—Ç—Ä–µ–±–æ–≤–∞—Ç—å—Å—è –Ω–∞—Å—Ç—Ä–æ–∏—Ç—å
            ActiveFunctions.AntiExplosion = enabled

            local player = game.Players.LocalPlayer
            if not player then return end

            if enabled then
                -- –ü–æ–¥–∫–ª—é—á–∞–µ–º –æ–±—Ä–∞–±–æ—Ç—á–∏–∫ StateChanged –ø—Ä–∏ –≤–∫–ª—é—á–µ–Ω–∏–∏
                local humanoid = player.Character and player.Character:FindFirstChild("Humanoid")
                if humanoid then
                    stateChangedConnection = humanoid.StateChanged:Connect(handleStateChanged)
                    print("Anti Explosion Enabled")
                    displayMessage("Anti Explosion Enabled", 2)
                else
                    warn("Humanoid not found for Anti Explosion.")
                    displayMessage("Anti Explosion: Humanoid not found.", 2)
                end
                -- –ò–∑–Ω–∞—á–∞–ª—å–Ω–æ —Å–æ—Ö—Ä–∞–Ω—è–µ–º —Ç–µ–∫—É—â—É—é –ø–æ–∑–∏—Ü–∏—é –∫–∞–∫ "—Ö–æ—Ä–æ—à—É—é"
                local root = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
                if root then
                    lastGoodPosition = root.Position
                end
            else
                -- –û—Ç–∫–ª—é—á–∞–µ–º –æ–±—Ä–∞–±–æ—Ç—á–∏–∫ StateChanged –ø—Ä–∏ –≤—ã–∫–ª—é—á–µ–Ω–∏–∏
                if stateChangedConnection then
                    stateChangedConnection:Disconnect()
                    stateChangedConnection = nil
                end
                lastGoodPosition = nil -- –°–±—Ä–∞—Å—ã–≤–∞–µ–º –ø–æ—Å–ª–µ–¥–Ω—é—é –ø–æ–∑–∏—Ü–∏—é
                print("Anti Explosion Disabled")
                displayMessage("Anti Explosion Disabled", 2)
            end
        end)
    end

    -- –§—É–Ω–∫—Ü–∏—è –æ–±—Ä–∞–±–æ—Ç–∫–∏ –Ω–∞–∂–∞—Ç–∏—è –∫–ª–∞–≤–∏—à
    local function setupKeyBindings()
        -- –°–æ–∑–¥–∞–µ–º –ø–æ–¥–∫–ª—é—á–µ–Ω–∏–µ –∫ —Å–æ–±—ã—Ç–∏—é –Ω–∞–∂–∞—Ç–∏—è –∫–ª–∞–≤–∏—à
        game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessed)
            -- –ù–µ –æ–±—Ä–∞–±–∞—Ç—ã–≤–∞–µ–º, –µ—Å–ª–∏ –∏–≥—Ä–∞ —É–∂–µ –æ–±—Ä–∞–±–æ—Ç–∞–ª–∞ –≤–≤–æ–¥ (–Ω–∞–ø—Ä–∏–º–µ—Ä, –≤ —á–∞—Ç–µ)
            if gameProcessed then return end

            -- –û–±—Ä–∞–±–æ—Ç–∫–∞ –∫–ª–∞–≤–∏—à–∏ G –¥–ª—è –∑–∞–∫—Ä—ã—Ç–∏—è/–æ—Ç–∫—Ä—ã—Ç–∏—è –º–µ–Ω—é
            if input.KeyCode == Enum.KeyCode.G then
                getgenv().KobraOpen = not getgenv().KobraOpen
                ScreenGui.Enabled = getgenv().KobraOpen
            end

            -- –û–±—Ä–∞–±–æ—Ç–∫–∞ –∫–ª–∞–≤–∏—à–∏ Tab –¥–ª—è —Å–ø–∞–≤–Ω–∞ –ø–∞–ª–ª–µ—Ç–∞
            if input.KeyCode == Enum.KeyCode.Tab and ActiveKeybinds.PalletSpawn then
                spawnPallet()
            end

            -- –û–±—Ä–∞–±–æ—Ç–∫–∞ –∫–ª–∞–≤–∏—à–∏ T –¥–ª—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏
            if input.KeyCode == Enum.KeyCode.T and ActiveKeybinds.ClickTeleport then
                teleportToMouse()
            end

            -- –û–±—Ä–∞–±–æ—Ç–∫–∞ –∫–ª–∞–≤–∏—à–∏ K –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è —è–∫–æ—Ä—è
            if input.KeyCode == Enum.KeyCode.K and ActiveKeybinds.AnchorGrabParts then
                createAnchor()
            end

            -- –û–±—Ä–∞–±–æ—Ç–∫–∞ –ø—Ä–æ–±–µ–ª–∞ –¥–ª—è –±–µ—Å–∫–æ–Ω–µ—á–Ω—ã—Ö –ø—Ä—ã–∂–∫–æ–≤
            if input.KeyCode == Enum.KeyCode.Space and ActiveFunctions.InfiniteJump then
                local player = game.Players.LocalPlayer
                local character = player.Character

                if character and character:FindFirstChild("Humanoid") then
                    character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
                end
            end
        end)
    end

    -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è –∏ –Ω–∞—Å—Ç—Ä–æ–π–∫–∏ —è–∫–æ—Ä—è (anchor)

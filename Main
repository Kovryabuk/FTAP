    -- –§—É–Ω–∫—Ü–∏—è —Å–æ–∑–¥–∞–Ω–∏—è –≤—ã–ø–∞–¥–∞—é—â–µ–≥–æ —Å–ø–∏—Å–∫–∞ –∏–≥—Ä–æ–∫–æ–≤ (–æ—Å—Ç–∞–ª–∞—Å—å –Ω–∞ –º–µ—Å—Ç–µ)
    local function createPlayerDropdown(parent, layoutOrder)
        local dropdownFrame = Instance.new("Frame")
        dropdownFrame.Size = UDim2.new(1, 0, 0, 40)
        dropdownFrame.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
        dropdownFrame.BorderSizePixel = 0
        dropdownFrame.LayoutOrder = layoutOrder
        dropdownFrame.Parent = parent
        dropdownFrame.ClipsDescendants = false

         -- –î–æ–±–∞–≤–ª—è–µ–º —Å–∫—Ä—É–≥–ª–µ–Ω–Ω—ã–µ —É–≥–ª—ã –∫ —Ä–∞–º–∫–µ –≤—ã–ø–∞–¥–∞—é—â–µ–≥–æ —Å–ø–∏—Å–∫–∞
        local FrameCorner = Instance.new("UICorner")
        FrameCorner.CornerRadius = UDim.new(0, 6)
        FrameCorner.Parent = dropdownFrame


        local dropdownButton = Instance.new("TextButton")
        dropdownButton.Size = UDim2.new(1, 0, 1, 0)
        dropdownButton.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
        dropdownButton.Text = "Select Player"
        dropdownButton.TextColor3 = Color3.new(1, 1, 1) -- –ß–∏—Å—Ç–æ –±–µ–ª—ã–π —Ü–≤–µ—Ç –¥–ª—è —Ç–µ–∫—Å—Ç–∞
        dropdownButton.TextSize = 16
        dropdownButton.Font = Enum.Font.Gotham
        dropdownButton.TextXAlignment = Enum.TextXAlignment.Left
        dropdownButton.TextYAlignment = Enum.TextYAlignment.Center -- –í—ã—Ä–∞–≤–Ω–∏–≤–∞–Ω–∏–µ –ø–æ —Ü–µ–Ω—Ç—Ä—É –ø–æ –≤–µ—Ä—Ç–∏–∫–∞–ª–∏
        dropdownButton.BorderSizePixel = 0
        dropdownButton.Parent = dropdownFrame

         -- –î–æ–±–∞–≤–ª—è–µ–º —Å–∫—Ä—É–≥–ª–µ–Ω–Ω—ã–µ —É–≥–ª—ã –∫ –∫–Ω–æ–ø–∫–µ –≤—ã–ø–∞–¥–∞—é—â–µ–≥–æ —Å–ø–∏—Å–∫–∞
        local ButtonCorner = Instance.new("UICorner")
        ButtonCorner.CornerRadius = UDim.new(0, 6)
        ButtonCorner.Parent = dropdownButton


        local dropdownList = Instance.new("ScrollingFrame")
        dropdownList.Size = UDim2.new(1, 0, 0, 150) -- –í—ã—Å–æ—Ç–∞ —Å–ø–∏—Å–∫–∞
        dropdownList.Position = UDim2.new(0, 0, 1, 0)
        dropdownList.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        dropdownList.BorderSizePixel = 1
        dropdownList.BorderColor3 = Color3.fromRGB(40, 40, 40)
        dropdownList.Visible = false
        dropdownList.ZIndex = 2 -- –ß—Ç–æ–±—ã —Å–ø–∏—Å–æ–∫ –±—ã–ª –ø–æ–≤–µ—Ä—Ö –¥—Ä—É–≥–∏—Ö —ç–ª–µ–º–µ–Ω—Ç–æ–≤
        dropdownList.Parent = dropdownFrame

         -- –î–æ–±–∞–≤–ª—è–µ–º —Å–∫—Ä—É–≥–ª–µ–Ω–Ω—ã–µ —É–≥–ª—ã –∫ —Å–ø–∏—Å–∫—É –≤—ã–ø–∞–¥–∞—é—â–µ–≥–æ —Å–ø–∏—Å–∫–∞
        local ListCorner = Instance.new("UICorner")
        ListCorner.CornerRadius = UDim.new(0, 6)
        ListCorner.Parent = dropdownList


        local listLayout = Instance.new("UIListLayout")
        listLayout.FillDirection = Enum.FillDirection.Vertical
        listLayout.SortOrder = Enum.SortOrder.LayoutOrder
        listLayout.Padding = UDim.new(0, 2) -- –î–æ–±–∞–≤–ª—è–µ–º –Ω–µ–±–æ–ª—å—à–æ–π –æ—Ç—Å—Ç—É–ø –º–µ–∂–¥—É –∫–Ω–æ–ø–∫–∞–º–∏
        listLayout.Parent = dropdownList

        local selectedPlayerName = nil

        -- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è —Å–ø–∏—Å–∫–∞ –∏–≥—Ä–æ–∫–æ–≤
        local function updatePlayerList()
            print("Updating player list...")
            -- –û—á–∏—Å—Ç–∏—Ç—å —Ç–µ–∫—É—â–∏–π —Å–ø–∏—Å–æ–∫
            for _, child in pairs(dropdownList:GetChildren()) do
                if child:IsA("TextButton") then
                    child:Destroy()
                end
            end

            local players = game.Players:GetPlayers()
            print("Found " .. #players .. " players.")
            local layoutOrder = 2
            local buttonHeight = 35 -- –£–≤–µ–ª–∏—á–∏–≤–∞–µ–º –≤—ã—Å–æ—Ç—É –∫–Ω–æ–ø–∫–∏
            local padding = listLayout.Padding.Offset

            for _, player in pairs(players) do
                -- –ü—Ä–æ–ø—É—Å–∫–∞–µ–º –ª–æ–∫–∞–ª—å–Ω–æ–≥–æ –∏–≥—Ä–æ–∫–∞
                if player ~= game.Players.LocalPlayer then
                    print("Adding player: " .. player.Name)
                    local playerButton = Instance.new("TextButton")
                    playerButton.Size = UDim2.new(1, 0, 0, buttonHeight) -- –ò—Å–ø–æ–ª—å–∑—É–µ–º –Ω–æ–≤—É—é –≤—ã—Å–æ—Ç—É
                    playerButton.BackgroundColor3 = Color3.fromRGB(70, 70, 70) -- –ù–µ–º–Ω–æ–≥–æ —Å–≤–µ—Ç–ª–µ–µ —Ñ–æ–Ω
                    playerButton.Text = player.Name
                    playerButton.TextColor3 = Color3.new(1, 1, 1) -- –ß–∏—Å—Ç–æ –±–µ–ª—ã–π —Ü–≤–µ—Ç –¥–ª—è —Ç–µ–∫—Å—Ç–∞
                    playerButton.TextSize = 16
                    playerButton.Font = Enum.Font.Gotham
                    playerButton.TextXAlignment = Enum.TextXAlignment.Left
                    playerButton.TextYAlignment = Enum.TextYAlignment.Center -- –í—ã—Ä–∞–≤–Ω–∏–≤–∞–Ω–∏–µ –ø–æ —Ü–µ–Ω—Ç—Ä—É –ø–æ –≤–µ—Ä—Ç–∏–∫–∞–ª–∏
                    playerButton.BorderSizePixel = 1 -- –î–æ–±–∞–≤–ª—è–µ–º –≥—Ä–∞–Ω–∏—Ü—É
                    playerButton.BorderColor3 = Color3.fromRGB(50, 50, 50) -- –¶–≤–µ—Ç –≥—Ä–∞–Ω–∏—Ü—ã
                    playerButton.LayoutOrder = layoutOrder
                    playerButton.Parent = dropdownList
                    playerButton.ZIndex = 2
                    playerButton.TextScaled = false -- –û—Ç–∫–ª—é—á–∞–µ–º –º–∞—Å—à—Ç–∞–±–∏—Ä–æ–≤–∞–Ω–∏–µ —Ç–µ–∫—Å—Ç–∞

                     -- –î–æ–±–∞–≤–ª—è–µ–º —Å–∫—Ä—É–≥–ª–µ–Ω–Ω—ã–µ —É–≥–ª—ã –∫ –∫–Ω–æ–ø–∫–µ –∏–≥—Ä–æ–∫–∞ –≤ —Å–ø–∏—Å–∫–µ
                    local PlayerButtonCorner = Instance.new("UICorner")
                    PlayerButtonCorner.CornerRadius = UDim.new(0, 6)
                    PlayerButtonCorner.Parent = playerButton


                    playerButton.MouseButton1Click:Connect(function()
                        selectedPlayerName = player.Name
                        dropdownButton.Text = "Selected: " .. player.Name
                        dropdownList.Visible = false -- –°–∫—Ä—ã—Ç—å —Å–ø–∏—Å–æ–∫ –ø–æ—Å–ª–µ –≤—ã–±–æ—Ä–∞
                    end)

                    layoutOrder = layoutOrder + 1
                end
            end
            -- –û–±–Ω–æ–≤–ª—è–µ–º CanvasSize –¥–ª—è –ø—Ä–æ–∫—Ä—É—Ç–∫–∏
            -- –£–±–µ–¥–∏–º—Å—è, —á—Ç–æ CanvasSize –¥–æ—Å—Ç–∞—Ç–æ—á–Ω–æ –±–æ–ª—å—à–æ–π –¥–ª—è –≤—Å–µ—Ö —ç–ª–µ–º–µ–Ω—Ç–æ–≤
            -- –£—á–∏—Ç—ã–≤–∞–µ–º –æ—Ç—Å—Ç—É–ø—ã –º–µ–∂–¥—É —ç–ª–µ–º–µ–Ω—Ç–∞–º–∏
            local totalHeight = (layoutOrder - 1) * (buttonHeight + padding)
            dropdownList.CanvasSize = UDim2.new(0, 0, 0, math.max(0, totalHeight))
             print("Player list updated. CanvasSize: " .. tostring(dropdownList.CanvasSize))
        end

        -- –û–±–Ω–æ–≤–ª—è–µ–º —Å–ø–∏—Å–æ–∫ –ø—Ä–∏ –æ—Ç–∫—Ä—ã—Ç–∏–∏ –≤—ã–ø–∞–¥–∞—é—â–µ–≥–æ —Å–ø–∏—Å–∫–∞
        dropdownButton.MouseButton1Click:Connect(function()
            dropdownList.Visible = not dropdownList.Visible
            if dropdownList.Visible then
                updatePlayerList()
            end
        end)

        -- –û–±–Ω–æ–≤–ª—è–µ–º —Å–ø–∏—Å–æ–∫ –ø—Ä–∏ –¥–æ–±–∞–≤–ª–µ–Ω–∏–∏/—É–¥–∞–ª–µ–Ω–∏–∏ –∏–≥—Ä–æ–∫–æ–≤
        game.Players.PlayerAdded:Connect(updatePlayerList)
        game.Players.PlayerRemoving:Connect(updatePlayerList)

        -- –ò–∑–Ω–∞—á–∞–ª—å–Ω–æ –∑–∞–ø–æ–ª–Ω—è–µ–º —Å–ø–∏—Å–æ–∫
        updatePlayerList()

        return {
            frame = dropdownFrame,
            getSelectedPlayer = function() return selectedPlayerName end
        }
    end


    -- –°–û–ó–î–ê–ù–ò–ï –í–ö–õ–ê–î–û–ö
    local mainTab = createTab("Main", 1)
